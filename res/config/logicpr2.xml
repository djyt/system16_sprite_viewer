<?xml version="1.0" encoding="utf-8"?>

<sprites>
    <!-- Similar format to System 16, but roms not interleaved. Endian different. -->
    <format>0</format>
    <!-- Override format interleave settings -->
    <interleave>1</interleave>
    <!-- Swap Endian of ROM -->
    <swap_endian>1</swap_endian>
    <roms>
        <rom name="roms/logicpr2/obj" offset="0x000000" length="0x400000"/>
    </roms>
    <!-- Sprite palette extracted from Main 68K Code at location 0x69354. Length 0x1640. -->
    <palette>
        <pal name="roms/logicpr2/logicpr2.pal" bytes_per_entry="32" offset="0"/>
    </palette>
</sprites>